# Redirect with a 301
/home         /                            301

# Redirect with a 302
/my-redirect  /                            302

# Rewrite a path
/pass-through /index.html                  200

# Invisbily proxy to a third party API
/api/*  https://api.example.com/:splat     200

# Show a custom 404 for a path
/special-offer    /too-late-its-over       404
